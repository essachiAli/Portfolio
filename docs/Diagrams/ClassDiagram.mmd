classDiagram
    class Developer {
        +string name
        +string title
        +string tagline
        +string bio
        +string email
        +string phone
        +string profileImage
        +getProfile(): array
        +getSkills(): Skill[]
        +getProjects(): Project[]
    }

    class Skill {
        +string name
        +string level
        +string category
        +string icon
    }

    class Project {
        +string title
        +string description
        +string link
        +string image
        +string technologies
        +getProjectDetails(): array
    }

    Developer "1" --> "*" Skill : has
    Developer "1" --> "*" Project : creates
